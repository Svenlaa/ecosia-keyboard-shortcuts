{
	"manifest_version": 2,
	"name": "Ecosia Keyboard Shortcuts",
	"version": "{{VERSION}}",
  
	"description": "Basic keyboard shortcuts for the Ecosia search results page.",
  
	"icons": {
	  "48": "icons/eks-icon.png"
	},
  
	"content_scripts": [
	  {
		"matches": ["*://*.ecosia.org/search*"],
		"js": ["ecosia_keyboard_shortcuts.js"]
	  }
	],
	"browser_specific_settings": {
	  "gecko": {
	    "id": "ecosia-keyboard-shortcuts"
	  }
	}

}

